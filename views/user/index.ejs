<% include ../partials/head.ejs %>
  <body>
    <% include ../partials/header.ejs %>
    <main>
    <div class="white">
      <div class="container">
        <div class="row">
          <div class="col s12 m6">
            <div class="card cyan darken-1">
              <div class="card-content white-text">
                <span class="card-title">Hello <%= user.firstName %>, your profile:</span>
                <p>User Id: <%= user.id %></p>
                <p>Username: <%= user.username %></p>
                <p>First Name: <%= user.firstName %></p>
                <p>Last Name: <%= user.lastName %></p>
                <p>Email: <%= user.email %></p>
              </div>
          </div>
        </div>
      </div>
      <a href="/auth/logout" class="btn-large waves-effect waves-light orange left">Logout</a>
      <a href="/movies" class="btn-large waves-effect waves-light orange left goToMoviesBtn">Folders</a>
      <a href="/directors" class="btn-large waves-effect waves-light orange left">Bookmarks</a>
      </div>
      </div>
    </main>
    <% include ../partials/footer.ejs %>
  </body>
</html>
